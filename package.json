{
  "name": "cdk-workshop",
  "version": "0.1.0",
  "bin": {
    "cdk-workshop": "bin/cdk-workshop.js"
  },
  "scripts": {
    "build": "tsc",
    "cdk": "cdk",
    "cdk:bootstrp": "cdk bootstrap",
    "cdk:deploy": "cdk deploy --require-approval never",
    "cdk:diff": "cdk diff",
    "cdk:synthesize": "cdk synth",
    "format": "prettier --write '**.ts' '.github/**/**.yaml'",
    "lint": "eslint --config eslint.config.js",
    "lint:fix": "pnpm lint --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:update-snapshots": "jest --updateSnapshot",
    "watch": "tsc -w"
  },
  "dependencies": {
    "aws-cdk-lib": "2.171.0",
    "aws-lambda": "^1.0.7",
    "constructs": "^10.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.146",
    "@types/jest": "^29.5.14",
    "@types/node": "22.7.9",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "aws-cdk": "2.171.0",
    "eslint": "9.16.0",
    "eslint-import-resolver-typescript": "3.7.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  }
}
